kila_karpou = int(input("Δώσε κιλά καρπού ελιάς: "))
kila_ladi = int(input("Δώσε κιλά λαδι: "))
arithmos_dendrwn = int(input("Δώσε αριθμό δέντρων: "))
dikaioma = int(input("Δικαίομα Εργοστασίου: "))
timi_polisis = float(input("Δώσε τιμή Πώλησης: "))

katharo1 = kila_ladi * dikaioma / 100
pososto = kila_ladi * 100 / kila_karpou
dentra = kila_karpou // arithmos_dendrwn
sakia = kila_karpou // 60
pososto2 = kila_ladi / sakia
katharo = kila_ladi - katharo1
timi = katharo * timi_polisis

print("=======================")
print(katharo, "Κιλά Λάδι, Καθαρό. ")
print(pososto, "% Ποσοστό λαδιού.")
print(dentra, "Κιλά Καρπού ανά δέντρο.")
print(sakia, "Σακιά σύνολο.")
print(pososto2, "Κιλά λάδι ανά Σακί.")
print(katharo1, "Κιλά Δικαίομα Εργοστασίου ")
print(timi, "€.")
